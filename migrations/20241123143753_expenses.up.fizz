create_table("expenses") {
    t.Column("id", "integer", {primary: true})
    t.Column("name", "string")
    t.Column("category", "string")
    t.Column("amount", "integer")
    t.Column("user_id", "integer", {})
}

add_foreign_key("expenses", "user_id", {"users": ["id"]}, {
    "on_delete": "cascade",
    "on_update": "cascade",
})